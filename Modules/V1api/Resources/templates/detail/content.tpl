<!-- パノラマのスクロールイベント //-->
<script>
$(function() {
  var $iframe = $('.panorama-frame');
  var $window = $(window);
  if ($iframe.length === 0) {
    console.log('no-panorama');
    return;
  }
  $window.on('scroll', autoloadIframe);

  function autoloadIframe() {
    var scrollBottom = $window.scrollTop() + $window.height();
    var iframeTop = $iframe.offset().top;

    if (scrollBottom > iframeTop) {
      $iframe.on('load', function() {
        // iframeの読み込み完了時にログ出力(ajax)
        // TODO: ログURLを決める

        $.ajax({
          type: 'GET',
          url: 'accesscount/panorama',
          dataType: 'json'
        }).then(
          function(data) {
            console.log(data);
          },
          function() {
            console.log('no-accesscount:panorama');
          }
        )
      });
      // iframeの読み込みを開始
      $iframe.attr('src', $iframe.attr('data-original'));
      // イベントハンドラを削除
      $window.off('scroll', autoloadIframe);
    }
  }
});
</script>
